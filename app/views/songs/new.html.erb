<h1>New Song Form</h1>
<%= form_for @song do |f|%>
  <% if @song.errors.any? %>
     <ul>
       <% @song.errors.full_messages do |message|%>
        <li><%= message %></li>
       <% end %>
     </ul>
  <% end %>
  
  <p>Title: <%= f.text.field :title %></p>
  <p>Released: <%= f.text.field :released %></p>
  <p>Release Year: <%= f.text.field :release_year %></p>
  <p>Genre: <%= f.text.field :genre %></p>
  <p>Artist Name: <%= f.text.field :artist_name %></p>

  <%= f.submit :'Create Song'%>
<% end %>
